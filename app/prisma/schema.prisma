generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Podcast {
  id        String    @id @default(cuid())
  name      String
  indexName String
  imageUrl  String
  episodes  Episode[]
  createdAt DateTime
}

model Episode {
  id              String   @id @default(cuid())
  backnumber      Int
  title           String
  description     String
  spotifyUrl      String
  applePodcastUrl String
  postedAt        DateTime
  podcast         Podcast  @relation(fields: [podcastId], references: [id])
  podcastId       String
}
